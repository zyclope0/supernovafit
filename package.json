{
  "name": "supernovafit",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "postinstall": "husky || true",
    "husky-install": "husky",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true && next build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:hooks": "vitest run src/hooks",
    "test:components": "vitest run src/components",
    "test:lib": "vitest run src/lib",
    "performance:budget": "node scripts/performance-budget.js",
    "performance:check": "npm run build && npm run performance:budget"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@sentry/nextjs": "^10.5.0",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.2",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^12.1.0",
    "fuse.js": "^7.1.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.303.0",
    "next": "^15.1.0",
    "next-pwa": "^5.6.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "web-vitals": "^5.1.0",
    "xml2js": "^0.6.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@next/bundle-analyzer": "^15.4.6",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.3.3",
    "ts-unused-exports": "^11.0.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
